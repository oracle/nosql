#expression returns second using Extract Function with timestamp of all nine precision
SELECT id,extract(second from t.ts0) as ts0,extract(second from t.ts1) as ts1,extract(second from t.ts2) as ts2,extract(second from t.ts3) as ts3,extract(second from t.ts4) as ts4,extract(second from t.ts5) as ts5,extract(second from t.ts6) as ts6,extract(second from t.ts7) as ts7 ,extract(second from t.ts8) as ts8 ,extract(second from t.ts9) as ts9 FROM Extract t
