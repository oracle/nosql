#expression returns month using Extract Function for non json array and order by id
SELECT id,extract(month from t.ats9[0]) as ats0,extract(month from t.ats9[1]) as ats1,extract(month from t.ats9[2]) as ats2,extract(month from t.ats9[3]) as ats3,extract(month from t.ats9[4]) as ats4,extract(month from t.ats9[5]) as ats5,extract(month from t.ats9[6]) as ats6,extract(month from t.ats9[7]) as ats7 ,extract(month from t.ats9[8]) as ats8 FROM Extract t ORDER BY id