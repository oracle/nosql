#expression returns month using Extract Function for non json map and order by id
SELECT id,extract(month from t.mts9.k1) as mts1,extract(month from t.mts9.k2) as mts2,extract(month from t.mts9.k3) as mts3,extract(month from t.mts9.k4) as mts4,extract(month from t.mts9.k5) as mts5,extract(month from t.mts9.k6) as mts6,extract(month from t.mts9.k7) as mts7 ,extract(month from t.mts9.k8) as mts8,extract(month from t.mts9.k9) as mts9 FROM Extract t ORDER BY Id 